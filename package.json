{
  "$schema": "https://json.schemastore.org/package",
  "name": "sync-with-template",
  "description": "Synchronize repository with template repository",
  "version": "3.0.0",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/main.js",
  "scripts": {
    "generate-config": "json2ts config.schema.json > src/internal/config.ts",
    "generate-local-transformations": "json2ts local-transformations.schema.json > src/internal/local-transformations.ts",
    "generate": "npm run generate-config && npm run generate-local-transformations",
    "lint": "eslint --fix",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run generate && npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir . --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/src/main.js --out dist",
    "build": "npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "@actions/glob": "0.3.0",
    "@actions/io": "1.1.2",
    "@octokit/plugin-request-log": "1.0.4",
    "@octokit/plugin-retry": "4.0.3",
    "@octokit/plugin-throttling": "4.3.2",
    "ajv": "8.12.0",
    "console-log-level": "1.4.1",
    "debug": "4.3.4",
    "picomatch": "2.3.1",
    "rimraf": "3.0.2",
    "simple-git": "3.15.1",
    "tmp": "0.2.1",
    "vm2": "3.9.13",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/picomatch": "2.3.0",
    "@types/rimraf": "3.0.2",
    "@types/tmp": "0.2.3",
    "@typescript-eslint/parser": "5.48.1",
    "@vercel/ncc": "0.36.0",
    "console-log-level": "1.4.1",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-github": "4.6.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.3.1",
    "jest-circus": "29.3.1",
    "json-schema-to-typescript": "11.0.2",
    "ts-jest": "29.0.4",
    "typescript": "4.9.4"
  }
}
