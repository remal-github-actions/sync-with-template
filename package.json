{
  "$schema": "https://json.schemastore.org/package",
  "name": "sync-with-template",
  "description": "Synchronize repository with template repository",
  "version": "3.0.0",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.3.1",
  "main": "dist/main.js",
  "scripts": {
    "generate-config": "json2ts config.schema.json > src/internal/config.ts",
    "generate-local-transformations": "json2ts local-transformations.schema.json > src/internal/local-transformations.ts",
    "generate": "npm run generate-config && npm run generate-local-transformations",
    "lint": "eslint --fix src/**/*",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run generate && npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir . --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/src/main.js --out dist",
    "build": "node check-dependency-engines && npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0",
    "@actions/glob": "0.4.0",
    "@actions/io": "1.1.3",
    "@octokit/plugin-request-log": "5.1.0",
    "@octokit/plugin-retry": "7.1.0",
    "@octokit/plugin-throttling": "8.2.0",
    "ajv": "8.16.0",
    "console-log-level": "1.4.1",
    "debug": "4.3.5",
    "json5": "2.2.3",
    "picomatch": "4.0.2",
    "rimraf": "5.0.8",
    "simple-git": "3.25.0",
    "tmp": "0.2.3",
    "vm2": "3.9.19",
    "yaml": "2.4.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.10",
    "@types/picomatch": "2.3.4",
    "@types/semver": "7.5.8",
    "@types/tmp": "0.2.6",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@vercel/ncc": "0.38.1",
    "console-log-level": "1.4.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-github": "5.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "json-schema-to-typescript": "14.1.0",
    "semver": "7.6.2",
    "ts-jest": "29.2.0",
    "typescript": "5.5.3"
  }
}
