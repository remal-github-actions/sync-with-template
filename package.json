{
  "$schema": "https://json.schemastore.org/package",
  "name": "sync-with-template",
  "description": "Synchronize repository with template repository",
  "version": "3.0.0",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@3.6.3",
  "main": "dist/main.js",
  "scripts": {
    "generate-config": "json2ts config.schema.json > src/internal/config.ts",
    "generate-local-transformations": "json2ts local-transformations.schema.json > src/internal/local-transformations.ts",
    "generate": "npm run generate-config && npm run generate-local-transformations",
    "lint": "eslint --fix --format compact",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run generate && npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir . --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/src/main.js --out dist",
    "build": "node check-dependency-engines && npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "5.1.1",
    "@actions/glob": "0.4.0",
    "@actions/io": "1.1.3",
    "@octokit/plugin-request-log": "4.0.0",
    "@octokit/plugin-retry": "5.0.5",
    "@octokit/plugin-throttling": "5.2.3",
    "ajv": "8.12.0",
    "console-log-level": "1.4.1",
    "debug": "4.3.4",
    "json5": "2.2.3",
    "picomatch": "2.3.1",
    "rimraf": "5.0.1",
    "simple-git": "3.20.0",
    "tmp": "0.2.1",
    "vm2": "3.9.19",
    "yaml": "2.3.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.2",
    "@types/jest": "29.5.5",
    "@types/node": "20.6.5",
    "@types/picomatch": "2.3.1",
    "@types/semver": "7.5.3",
    "@types/tmp": "0.2.4",
    "@typescript-eslint/parser": "6.7.3",
    "@vercel/ncc": "0.38.0",
    "console-log-level": "1.4.1",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-github": "4.10.0",
    "eslint-plugin-jest": "27.4.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "json-schema-to-typescript": "13.1.1",
    "semver": "7.5.4",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
